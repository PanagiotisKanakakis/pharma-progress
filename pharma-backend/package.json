{
  "name": "PHARMA",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build-webpack": "nest build --webpack --webpackPath webpack.config.js ",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "schema:sync": "ts-node node_modules/typeorm/cli.js schema:sync -f src/database/config/migrations.pgconfig.ts",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f src/database/config/migrations.pgconfig.ts -p -n",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run -f src/database/config/migrations.pgconfig.ts",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f src/database/config/migrations.pgconfig.ts",
    "migration:show": "ts-node node_modules/typeorm/cli.js migration:show -f src/database/config/migrations.pgconfig.ts",
    "seeds:create": "ts-node node_modules/typeorm/cli.js migration:create -f src/database/config/seeds.pgconfig.ts -n",
    "seeds:run": "ts-node node_modules/typeorm/cli.js migration:run -f src/database/config/seeds.pgconfig.ts",
    "seeds:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f src/database/config/seeds.pgconfig.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@golevelup/nestjs-common": "^1.4.3",
    "@keycloak/keycloak-admin-client": "^17.0.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "0.0.7",
    "@nestjs/bull": "^0.5.3",
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/platform-express": "^8.4.1",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "bull": "^4.5.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "ioredis": "^4.28.5",
    "keycloak-connect": "^17.0.0",
    "luxon": "^2.3.1",
    "ng2-date-picker": "^13.1.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.1",
    "@types/amqplib": "^0.8.2",
    "@types/bull": "^3.15.8",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/luxon": "^2.3.1",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-mock": "^27.5.1",
    "prettier": "^2.6.0",
    "run-script-webpack-plugin": "0.0.11",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
